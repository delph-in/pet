dont-rule := tokmap-rule &
  [ CONTEXT < [ FORM "do",
                FROM #1,
                TO   #2        ] > ,
    INPUT   < [ FORM "n't",
                FROM #2,
                TO   #3        ] > ,
    OUTPUT  < [ FORM "don't",
                FROM #1,
                TO   #3        ] > ].

dag-rule := tokmap-rule &
  [ CONTEXT < [ FORM "the"] > ,
    INPUT   < [ FORM "dag"] > ,
    OUTPUT  < [ FORM "dog"] > ].

