;;;
;;; Lexical filtering rules (chart mapping)
;;;

; throw out generic nouns where native nouns are available:
filter-generic-nouns := lexmap-rule &
  [ +CONTEXT < noun-lxm & [ ORTH <! "/(?!generic-stem).*/" !> ] > ,
    +INPUT   < noun-lxm & [ ORTH <! "generic-stem" !> ] > ,
    +OUTPUT  <> ,
    +POSITION "I1@C1" ].

